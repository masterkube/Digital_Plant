<?xml version="1.0" encoding="UTF-8"?>
<Agent>
	<AgentName>AssemblyAgent</AgentName>
	<ProcessName>Assemblies</ProcessName>
	<AvatarName>UniqueID</AvatarName>
	<Element>GetBackToModel</Element>
	<Element>MakeSubAssembly</Element>
	<Element>UniqueID</Element>
	<Compose>
		<Tell><action><actionName>AssemblyInfo</actionName></action></Tell>
		<Ask><action><actionName>ConnectWithAssembly</actionName></action><internal>true</internal></Ask>
		<Tell><element><elementName>GetBackToModel</elementName></element></Tell>
		<Tell><action><actionName>GenUniqueId</actionName></action><internal>true</internal></Tell>
		<Sequence>
			<Ask><action><actionName>GenUniqueId</actionName></action><internal>true</internal></Ask>
			<Tell><element><elementName>MakeSubAssembly</elementName></element></Tell>
			<Tell><action><actionName>NextSubAssembly</actionName></action></Tell>
        </Sequence>
		<Sequence>
			<Ask><action><actionName>Connected</actionName></action><internal>true</internal></Ask>
			<Ask><action><actionName>PickAssembly</actionName></action><internal>true</internal></Ask>
			<Tell><element><elementName>GetNameOfTheAssembly</elementName></element></Tell>
		</Sequence>
	</Compose>
</Agent>
